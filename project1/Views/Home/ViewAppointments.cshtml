@{
    ViewData["Title"] = "View Appointment";
}
@model IEnumerable<Project>

<div class="card-text p-1">
    <h2>View Appointments</h2>
</div>


<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Group Name</th>
            <th>Group Size</th>
            <th>Time</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var x in Model)
        {
        <tr>
            <td class="text-left">@x.GroupName</td>
            <td class="text-center">@x.GroupSize</td>
            <td class="text-right">@x.DayTime</td>
        </tr>
        }
    </tbody>
</table>